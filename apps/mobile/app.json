    },
    "extra": {
      "eas": {
        "projectId": "ac2b1e7b-e5fb-4bff-9f7c-f0f840e90c5a"
      }
    },
    "owner": "adlgo-app",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ac2b1e7b-e5fb-4bff-9f7c-f0f840e90c5a"
    }
  }
